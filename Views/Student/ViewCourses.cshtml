@model ICollection<Course>
<h5>Index Page</h5>
<table class="table table-hover mt-5">
    <thead>
        <tr><th>Course Name</th>
            <th>Course Code</th>
            <th>Designation</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        { <tr>
                <td>@item.CourseName</td>
                <td>@item.CourseCode</td>
                <td>@item.SchoolLevel.LevelName</td> 
            </tr>
        }
    </tbody>
</table>